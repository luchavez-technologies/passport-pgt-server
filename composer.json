{
    "name": "luchavez/passport-pgt-server",
    "description": "Laravel Passport Password Grant Tokens Server for Laravel 8|9|10",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarloluchavez@gmail.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez"
        }
    ],
    "homepage": "https://github.com/luchavez-technologies/passport-pgt-server",
    "keywords": ["Laravel", "PassportPgtServer", "OAuth2"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~8|~9|~10",
        "laravel/passport": "^10.4|^11.5",
        "luchavez/passport-pgt-client": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~6|~7|~8"
    },
    "autoload": {
        "psr-4": {
            "Luchavez\\PassportPgtServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luchavez\\PassportPgtServer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luchavez\\PassportPgtServer\\Providers\\PassportPgtServerServiceProvider"
            ],
            "aliases": {
                "PassportPgtServer": "Luchavez\\PassportPgtServer\\Facades\\PassportPgtServer"
            }
        },
        "branch-alias": {
            "dev-master": "2.0.x-dev",
            "dev-develop": "2.0.x-dev"
        }
    }
}
